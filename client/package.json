    "format": "prettier --write src/"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.9.0",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.5.1",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^3.8.7"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.15",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.89.2",
    "typescript": "~5.1.6",
    "vite": "^6.3.5",
